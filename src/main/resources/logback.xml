<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>-> %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="net.bunselmeyer.tsd" level="INFO"/>

    <root level="warn">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>